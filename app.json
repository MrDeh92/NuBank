{
  "expo": {
    "name": "Nubank",
    "slug": "Nubank",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Nuicon.png",
    "splash": {
      "image": "./assets/Nusplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8B10AE"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Nuadaptive-icon.png",
        "backgroundColor": "#8B10AE"
      }
    },
    "web": {
      "favicon": "./assets/Nufavicon.png"
    }
  }
}
